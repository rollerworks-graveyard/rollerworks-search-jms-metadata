{
    "name": "rollerworks/search-jms-metadata",
    "description": "JMS Metadata extension for RollerworksSearch",
    "keywords": ["rollerworks", "search", "metadata"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/rollerworks-search-jms-metadata/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rollerworks/search": ">=1.0.0-beta3@dev",
        "doctrine/annotations": "~1.1",
        "jms/metadata": "~1.4"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1@dev"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Metadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Metadata\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
